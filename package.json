{
  "name": "template-project",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:all": "next dev & npx supabase start & stripe listen --forward-to localhost:3000/api/webhooks/stripe",
    "dev:be": "yarn mailpit:start & npx supabase start & stripe listen --forward-to localhost:3000/api/webhooks/stripe",
    "build": "next build",
    "start": "next start",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx,css,html}\"",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop",
    "db:status": "npx supabase status",
    "db:reset": "npx supabase db reset",
    "stripe:listen": "stripe listen --forward-to localhost:3000/api/webhooks/stripe",
    "stripe:events": "stripe listen --forward-to localhost:3000/api/webhooks/stripe --events payment_intent.succeeded,customer.subscription.created,customer.subscription.updated,customer.subscription.deleted,invoice.payment_succeeded,invoice.payment_failed",
    "stripe:secret": "stripe listen --print-secret",
    "mailpit:start": "docker run -d --name mailpit -p 1025:1025 -p 8025:8025 axllent/mailpit || docker start mailpit",
    "mailpit:stop": "docker stop mailpit"
  },
